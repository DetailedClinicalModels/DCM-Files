><UML:ModelElement.stereotype><UML:Stereotype name="rootconcept"/></UML:ModelElement.stereotype><UML:ModelElement.taggedValue><UML:TaggedValue tag="documentation" value="De simms-classificatie is een risico-inventarisatie die in kaart brengt welke risicos de patiënt loopt op het mogelijk ontstaan van een ulcus of meerdere ulcera (Provoet, 2012)."/><UML:TaggedValue tag="isSpecification" value="false"/><UML:TaggedValue tag="ea_stype" value="Class"/><UML:TaggedValue tag="ea_ntype" value="0"/><UML:TaggedValue tag="version" value="1.0"/><UML:TaggedValue tag="package" value="EAPK_D2AC48D0_8286_48ed_A056_EC6EAD98717E"/><UML:TaggedValue tag="date_created" value="2010-01-08 13:55:16"/><UML:TaggedValue tag="date_modified" value="2013-06-13 10:39:21"/><UML:TaggedValue tag="gentype" value="Java"/><UML:TaggedValue tag="tagged" value="0"/><UML:TaggedValue tag="package_name" value="Information Model"/><UML:TaggedValue tag="phase" value="1.0"/><UML:TaggedValue tag="author" value="ZelM"/><UML:TaggedValue tag="complexity" value="1"/><UML:TaggedValue tag="status" value="Proposed"/><UML:TaggedValue tag="stereotype" value="rootconcept"/><UML:TaggedValue tag="tpos" value="0"/><UML:TaggedValue tag="ea_localid" value="6039"/><UML:TaggedValue tag="ea_eleType" value="element"/><UML:TaggedValue tag="style" value="BackColor=-1;BorderColor=-1;BorderWidth=3;FontColor=-1;VSwimLanes=1;HSwimLanes=1;BorderStyle=0;"/><UML:TaggedValue tag="$ea_xref_property" value="$XREFPROP=$XID={26BCBE46-FC29-4c8a-8D7A-4D94077A52A0}$XID;$NAM=Stereotypes$NAM;$TYP=element property$TYP;$VIS=Public$VIS;$PAR=0$PAR;$DES=@STEREO;Name=rootconcept;GUID={FF7F2377-ADFA-4355-9F4E-BA247373D156};@ENDSTEREO;$DES;$CLT={D8A87307-DABB-4058-A848-42D22C2EDA29}$CLT;$ENDXREF;$XREFPROP=$XID={CC2F3BBC-074F-4c93-A9EA-D10921E0945C}$XID;$NAM=CustomProperties$NAM;$TYP=element property$TYP;$VIS=Public$VIS;$PAR=0$PAR;$DES=@PROP=@NAME=isActive@ENDNAME;@TYPE=Boolean@ENDTYPE;@VALU=@ENDVALU;@PRMT=@ENDPRMT;@ENDPROP;$DES;$CLT={D8A87307-DABB-4058-A848-